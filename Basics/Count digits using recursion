import java.util.Scanner;
public class main{
    public static int Num(int n )
    {
       if(n==0)
       {
        return 0;
       }
       return 1 + Num(n/10);
      
    }
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(Num(n));
        sc.close();

    }
}
